Public Class Converter

    Public Shared RTFHeader As String = RTF.ヘッダ
    Public Shared RTFFooter As String = RTF.フッタ
    Private Shared ht As Hashtable = Nothing

    Public Shared ReadOnly Property Hashtable As Hashtable
        Get
            If ht Is Nothing Then

                ' 変換用テーブル
                ' ・ボタンクリックを RTF に変換
                ' ・文字実体参照を RTF に変換
                ' ・HTML を文字実体参照にエスケープ
                ' ・エスケープされた文字を戻す
                ht = New Hashtable()

                ht.Add("ブタ", RTF.ブタ)
                ht.Add("会話", RTF.会話)
                ht.Add("回転", RTF.回転)
                ht.Add(CR.ブタ, RTF.ブタ)
                ht.Add(CR.会話, RTF.会話)
                ht.Add(CR.回転, RTF.回転)

                ht.Add(HTML.アンパサンド, CR.アンパサンド)
                ht.Add(HTML.小なり, CR.小なり)
                ht.Add(HTML.大なり, CR.大なり)
                ht.Add(CR.アンパサンド, HTML.アンパサンド)
                ht.Add(CR.小なり, HTML.小なり)
                ht.Add(CR.大なり, HTML.大なり)

                ht.Add(vbLf, HTML.改行)
                ht.Add(HTML.改行, vbLf)

                ht.Add(CSV.カンマ, CR.カンマ)
                ht.Add(CR.カンマ, CSV.カンマ)
            End If

            Return ht
        End Get
    End Property

    Public Shared Function Emphasize(ByVal str As String) As String
        ' 強調表示(赤太字)
        Return HTML.強調開始 & str & HTML.強調終了
    End Function

    Public Shared Function Simplify(ByVal str As String) As String
        ' Emphasize が一文字ずつタグで囲むため、
        ' タグの終了と開始が連続している部分を除去して Text を簡単にする
        Return str.Replace(HTML.強調終了 & HTML.強調開始, String.Empty)
    End Function

    Public Shared Function IsBR(ByVal str As String) As Boolean
        ' 改行タグである
        Return str.Equals(HTML.改行)
    End Function

    Public Shared Function IsStartEM(ByVal str As String) As Boolean
        ' 強調表示の開始タグである
        Return str.Equals(HTML.強調開始)
    End Function

    Public Shared Function Escape(ByVal str As String) As String
        ' ユーザの入力と変換後の文字を区別するため、
        ' ユーザ入力をエスケープする
        Dim r As String = str
        If Hashtable.ContainsKey(str) Then
            r = ht(str)
        End If
        Return r
    End Function

    Public Shared Function ToCharacterReference(ByVal _rtf As String) As String

        ' RTF で保持している画像のうち画素に相当する部分のみでパターンマッチし、
        ' 文字実体参照に変換する
        _rtf = _rtf.Replace(vbCrLf, String.Empty)
        Dim _cr As String = String.Empty

        If _rtf.Contains(Key.ブタ) Then
            _cr = CR.ブタ
        ElseIf _rtf.Contains(Key.会話) Then
            _cr = CR.会話
        ElseIf _rtf.Contains(Key.回転) Then
            _cr = CR.回転
        End If

        Return _cr
    End Function

    Private Class CSV
        Public Shared カンマ As String = ","
    End Class

    Private Class HTML
        ' いわゆる HTML タグ
        Public Shared 強調開始 As String = "<strong>"
        Public Shared 強調終了 As String = "</strong>"
        Public Shared アンパサンド As String = "&"
        Public Shared 小なり As String = "<"
        Public Shared 大なり As String = ">"
        Public Shared 改行 As String = "<br>"
    End Class

    Private Class CR
        ' 文字実体参照(&amp;, &lt;, &gt; 以外は独自の表記であることに注意)
        Public Shared アンパサンド As String = "&amp;"
        Public Shared 小なり As String = "&lt;"
        Public Shared 大なり As String = "&gt;"

        ' 文字実体参照(独自の表記)
        Public Shared ブタ As String = "&pig;"
        Public Shared 会話 As String = "&cnv;"
        Public Shared 回転 As String = "&recl;"

        ' 数値実体参照
        Public Shared カンマ As String = "&#44;"
    End Class

    Private Class RTF
        ' RTF を ASCII で保持するためのもの
        ' ヘッダとフッタではさんだものをテキストファイルに貼り付ければ
        ' ワードパッド等で閲覧できるようになる。
        Public Shared ヘッダ As String = "{\rtf1\ansi\ansicpg932\deff0\deflang1033\deflangfe1041{\fonttbl{\f0\fnil\fcharset128 MS UI Gothic;}}\viewkind4\uc1\pard\lang1041\f0\fs18"
        Public Shared フッタ As String = "}"

        Public Shared ブタ As String = "{\pict\picscalex101\picscaley101\piccropl0\piccropr0\piccropt0\piccropb0\picw1129\pich1129\picwgoal640\pichgoal640\wmetafile8\bliptag1339236142\blipupi72{\*\blipuid 4fd31f2e9cdd94b84e319524f837e2af}}"
        Public Shared 会話 As String = "{\pict\picscalex101\picscaley101\piccropl0\piccropr0\piccropt0\piccropb0\picw1129\pich1129\picwgoal640\pichgoal640\wmetafile8\bliptag-1781194001\blipupi72{\*\blipuid 95d522ef32c73c1823203848e258efeb}}"
        Public Shared 回転 As String = "{\pict\picscalex101\picscaley101\piccropl0\piccropr0\piccropt0\piccropb0\picw1129\pich1129\picwgoal640\pichgoal640\wmetafile8\bliptag-2059759893\blipupi72{\*\blipuid 853a8eeb93c8514120ef1f8538f626a6}}"
    End Class

    Private Class Key
        ' RTF のうち、拡大率などユーザの操作によって変更を受ける部分を除いた文字列
        ' (画像自体が記述されている部分)
        Public Shared ブタ As String = RTF.ブタ.Split(New Char() {" "c, "}"c})(2)
        Public Shared 会話 As String = RTF.会話.Split(New Char() {" "c, "}"c})(2)
        Public Shared 回転 As String = RTF.回転.Split(New Char() {" "c, "}"c})(2)
    End Class
End Class
